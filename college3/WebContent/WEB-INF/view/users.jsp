    function emailalert() {
        if ($('#Institution_users option:selected').val() == 1) {
            $('#overlaygif').show("slow");
            $.ajax({
                type: "POST",
                url: "/index.php/sms/sendmail/useremailalert",
                data: {user: $('#Institution_users option:selected').val(), courseid: $('#Institution_courseid option:selected').val(), batchid: $('#Institution_batchid option:selected').val()},
                dataType: "html",
                success: function (data) {
                    $('#overlaygif').hide("slow");
                    if (data == "error") {
                        alert("Please configure smtp email settings");
                    } else {
                        alert("Successfully send");
                        location.reload();
                    }
                }
            });
        }
        if ($('#Institution_users option:selected').val() == 2) {
            $('#overlaygif').show("slow");
